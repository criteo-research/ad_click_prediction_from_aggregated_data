   "source": [
    "def naiveHash(string:str):\n",
    "    # Too simple, not advised in real applications! \n",
    "    # Instead, use eg \"fnv32\" or \"murmurhash\"\n",
    "    h = 1\n",
    "    for c in string:\n",
    "        i =  ord(c) # convert character c to interger\n",
    "        h = 31 * h + i\n",
    "    return h"
   ]
